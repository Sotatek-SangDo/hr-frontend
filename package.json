{
  "name": "hrfront",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "lint": "vue-cli-service lint",
    "start": "cross-env NODE_ENV=production node server",
    "fix-and-test": "eslint --fix --ext .jsx,.js,.vue ./src/components",
    "dev": "node server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "rimraf test/e2e/reports && cross-env NODE_ENV=testing node test/e2e/runner.js",
    "test:unit": "rimraf test/unit/coverage && cross-env NODE_ENV=testing karma start test/unit/karma.conf.js --single-run"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.0.0",
    "bootstrap-datepicker": "^1.8.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "client-oauth2": "^4.2.3",
    "compression": "^1.7.0",
    "datatables": "^1.10.18",
    "datatables-bootstrap": "0.0.1",
    "datatables.net-bs4": "^1.10.19",
    "datatables.net-responsive": "^2.2.3",
    "es6-promise": "^4.1.1",
    "express": "^4.16.4",
    "jquery": "^3.3.1",
    "metismenu": "^2.7.8",
    "node-sass": "^4.10.0",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.4.0",
    "popper.js": "^1.14.5",
    "sass-loader": "^7.1.0",
    "simple-oauth2": "^2.2.1",
    "vue": "^2.5.17",
    "vue-authenticate": "^1.3.4",
    "vue-axios": "^2.1.4",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.22",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@prerenderer/renderer-jsdom": "^0.2.0",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "@vue/eslint-config-prettier": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "prerender-spa-plugin": "^3.4.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
